algoritmo "semnome"
var
n: inteiro
inicio
escreva ("digite um numero aleatorio: ")
leia (n)
se (n>0)entao
   escreva ("este numero é positivo")
senao
   se (n=0)entao
      escreva ("este numero e nulo")
   senao
      se (n<0)entao
         escreva ("este numero e negativo")
      senao
      fimse
   fimse
fimse

fimalgoritmo

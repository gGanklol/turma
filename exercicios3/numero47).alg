algoritmo "numero47"
var
n1,n2 :inteiro
r:real
inicio
escreva ("informe um número: ")
leia(n1)
escreva ("informe outro número: ")
leia (n2)
enquanto (n2=0)faca
      escreval ("VÁLOR INVÁLIDO")
      escreva ("informe outro número: ")
      leia (n2)
fimenquanto
r:= (n1/n2)
escreva (r , " é o resultado final")
fimalgoritmo
